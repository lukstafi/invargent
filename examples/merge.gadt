newtype Ordered : num * num
newtype OList : num * num
newtype Num : num

newcons Leq : ∀a, b) with a <= b: Ordered (a, b)
newcons Geq : ∀a, b) with b <= a: Ordered (a, b)

external compare : ∀c, d. Num(c) → Num(d) → Ordered (c, d)

newcons ONil : OList(0, 0)
newcons OCons :
  ∀n, a, b) with b <= a: Num(a) * OList(n, b) ⟶ OList(n+1, a)


let rec merge =
  function ONil -> (fun l -> l)
    | OCons (a, l1) as l ->
	function ONil -> l
	  | OCons (b, l3) as l2 ->
	      match compare a b with
		  Leq -> OCons (a, merge l1 l2)
		| Geq -> OCons (b, merge l l3)
