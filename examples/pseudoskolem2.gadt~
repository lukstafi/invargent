newtype Bar
newcons Foo : Bar

newtype Count
newtype List : nat

newcons One : Count
newcons Two : Count
newcons Three : Count
newcons LNil : List 0
newcons LCons : for all (n) : Bar * List(n) âŸ¶ List(n+1)

let rec this_could_typecheck =
  let afoo = function One -> LCons (Foo, LNil)
    | Two -> LCons (Foo, LCons (Foo, LNil))
    | Three -> LCons (Foo, LCons (Foo, LCons (Foo, LNil)))
  in afoo Two