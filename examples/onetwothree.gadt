newtype Bar
newcons Foo : Bar

newtype Count
newtype List : num

newcons One : Count
newcons Two : Count
newcons Three : Count
newcons LNil : List 0
newcons LCons : ∀n. Bar * List(n) ⟶ List(n+1)

let rec at_least_one =
  function One -> LCons (Foo, LNil)
    | Two -> LCons (Foo, LCons (Foo, LNil))
    | Three -> LCons (Foo, LCons (Foo, LCons (Foo, LNil)))
